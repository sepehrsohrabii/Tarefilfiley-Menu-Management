{
  "expo": {
    "name": "menuManagement",
    "slug": "menuManagement",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LOGO.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LOGO.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sepehrsohrabii.menuManagement"
    },
    "web": {
      "favicon": "./assets/LOGO.png"
    },
    "expo": {
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you put it in your menu.",
            "cameraPermission": "The app accesses your camera to let you take picture of your product and put it in your menu."
          }
        ]
      ]
    },
    "extra": {
      "eas": {
        "projectId": "94a59e60-0beb-4236-974c-ee5a525e3176"
      }
    }
  }
}
